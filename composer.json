{
    "name": "rh/rh-majordomo",
    "type": "typo3-cms-extension",
    "description": "It allows to add a FE-Users himself to an Majordomo Email-List.",
    "keywords": [
        "TYPO3",
        "extension",
        "majordomo"
    ],
    "authors": [
        {
            "name": "Rolf Huesmann",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rh\\RhMajordomo\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rh\\RhMajordomo\\Tests\\": "Tests"
        }
    },
    "support": {
      "issues": "https://github.com/Rolfff/rh_majordomo/issues",
      "source": "https://github.com/Rolfff/rh_majordomo",
      "docs": "https://github.com/Rolfff/rh_majordomo/blob/master/README.md"
   },
    "replace": {
        "rh/rh-majordomo": "self.version",
        "typo3-ter/rh-majordomo": "self.version"
    },
     "extra": {
        "typo3/cms": {
            "extension-key": "rh_majordomo"
        }
    }
}
